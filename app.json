{
  "expo": {
    "name": "Vibe",
    "slug": "vibe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vibe-icon.png",
    "scheme": "vibe",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.piluso.vibe"
    },
    "android": {
      "package": "com.piluso.vibe",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vibe-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAsyL4SfX9ucJuaZj158G9OcVvS6bU0r3A"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-location",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SpaceMono-Regular.ttf"
          ]
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "paymentDescription": "Vibe Premium",
          "merchantIdentifier": "merchant.com.piluso.vibe"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c656a465-f7a4-4ba2-89c2-c6fadec34dd3"
      }
    }
  }
}
